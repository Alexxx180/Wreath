<Application x:Class="Wreath.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Wreath"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Total.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Styles/Buttons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Styles/TextBoxes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Styles/ComboBoxes.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
